<p id="notice"><%= notice %></p>

<h1>Listing Videos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Duration</th>
      <th>Embed code</th>
      <th>Description</th>
      <th>Topic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @videos.each do |video| %>
      <tr>
        <td><%= video.title %></td>
        <td><%= video.duration %></td>
        <td><%= video.embed_code %></td>
        <td><%= video.description %></td>
        <td><%= video.topic %></td>
        <td><%= link_to 'Show', "#" %></td>
        <td><%= link_to 'Edit', "#" %></td>
        <td><%= link_to 'Destroy', "#", method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Video', "#" %>
